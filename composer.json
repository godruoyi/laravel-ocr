{
    "name": "godruoyi/laravel-ocr",
    "description": "The Best Image Ocr SDK For BAT.",
    "keywords": ["ocr", "ocr sdk", "laravel ocr", "php ocr"],
    "homepage": "https://github.com/godruoyi/ocr",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "godruoyi",
            "email": "godruoyi@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "godruoyi/ocr": "^3.0",
        "illuminate/support": "^10.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Godruoyi\\LaravelOCR\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Godruoyi\\LaravelOCR\\OCRServiceProvider"
            ],
