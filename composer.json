{
    "name": "godruoyi/laravel-ocr",
    "description": "The Best Image Ocr SDK For BAT.",
    "keywords": ["ocr", "ocr sdk", "laravel ocr", "php ocr"],
    "homepage": "http://github.com/godruoyi/ocr",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "godruoyi",
            "email": "godruoyi@gmail.com"
        }
    ],
    "require": {
        "godruoyi/ocr": "^2.0",
        "illuminate/framework": "^5.1 || ^6.0 || ^7.0 || ^8.0",
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Godruoyi\\LaravelOCR\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Godruoyi\\LaravelOCR\\OCRServiceProvider"
            ],
            "aliases": {
                "OCR": "Godruoyi\\LaravelOCR\\OCR"
            }
        }
    }
}
